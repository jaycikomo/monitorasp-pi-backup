                        if (result.online) {
                            resultDiv.innerHTML = `âœ… En ligne (${result.time}ms)`;
                            resultDiv.style.color = '#27ae60';
                            onlineCount++;
                        } else {
                            resultDiv.innerHTML = 'âŒ Hors ligne';
                            resultDiv.style.color = '#e74c3c';
