    <script>
        function testCamera(ip, camId) {
            const resultDiv = document.getElementById(`test-result-${camId}`);
            resultDiv.innerHTML = 'ğŸ”„ Test en cours...';
            resultDiv.style.color = '#f39c12';
            
            fetch(`http://192.168.1.200:3000/api/ping-camera/${ip}`)
                .then(response => {
                    if (!response.ok) {
                        throw new Error('Network response was not ok');
                    }
                    return response.json();
                })
                .then(data => {
                    console.log('RÃ©sultat ping:', data);
                    if (data.success && data.status === 'online') {
                        resultDiv.innerHTML = `âœ… En ligne (${data.time}ms)`;
                        resultDiv.style.color = '#27ae60';
                    } else {
                        resultDiv.innerHTML = `âŒ Hors ligne (${data.error || 'Pas de rÃ©ponse'})`;
                        resultDiv.style.color = '#e74c3c';
                    }
                })
                .catch(error => {
                    console.error('Erreur fetch:', error);
                    resultDiv.innerHTML = 'âŒ Erreur de test';
                    resultDiv.style.color = '#e74c3c';
                });
        }

